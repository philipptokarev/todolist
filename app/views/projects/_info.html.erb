<div class="project-name">
  <i class="fas fa-calendar-alt"></i>
  <%= project.name %>
</div>
<div class="project-links">
  <%= link_to edit_user_project_path(user_id: current_user.id, id: project.id), remote: true do %>
    <i class="fas fa-pen"></i>
  <% end%>
  <%= link_to user_project_path(user_id: current_user.id, id: project.id), method: :delete, remote: true, class: "delete_project" do %>
    <i class="fas fa-trash-alt"></i>
  <% end %>
</div>
